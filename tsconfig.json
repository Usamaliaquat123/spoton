{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "isolatedModules": true,
      "jsx": "react-native",
      "lib": ["es2017"],
      "types": ["react-native", "jest"],
      "moduleResolution": "node",
      "noEmit": true,
      "strict": true,
      "baseUrl": "src",
      "target": "esnext",
      "paths": {
        "@nav" :["./navigation/index"],
        "@nav/*" :["./navigation/*"],
        "@theme/*" :["./theme/*"],
        "@theme" :["./theme/index"],
        "@components" :["./components/index"],
        "@components/*" :["./components/*"],
        "@containers" :["./containers/index"],
        "@service/*":["./services/*"], 
        "@service":["./services/index"], 
        "@utils/*":["./utils/*"], 
        "@utils":["./utils/index"], 
        "@store/*":["./store/*"], 
        "@store":["./store/index"], 
        "@deco":["./decorators/index"],
        "@typings":["./typings/index"],
        "@config":["./config"],
        "@config/*":["./config/*"],
        "@dtos":["./dtos/index"],
        "@dtos/*":["./dtos/*"],
        "@actions":["./store/actions/index"],
        "@actions/*":["./store/actions/*"],
      }
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js"
    ]
}
